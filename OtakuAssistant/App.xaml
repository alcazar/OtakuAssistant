<Application
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:OtakuAssistant"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:SampleData="using:Blend.SampleData.SampleWord" mc:Ignorable="d"
    x:Class="OtakuAssistant.App"
    RequestedTheme="Dark">
    <Application.Resources>
        <SampleData:SampleWord x:Key="SampleWord" d:IsDataSource="True"/>
        <local:SearchViewHanziSize x:Key="SearchViewHanziSize"/>
        <local:SearchViewHanziWrapper x:Key="SearchViewHanziWrapper"/>
        <local:WordViewHanziSize x:Key="WordViewHanziSize"/>
        <local:WordViewHanziWrapper x:Key="WordViewHanziWrapper"/>
        <local:WordViewPinyinLine x:Key="WordViewPinyinLine"/>
    </Application.Resources>
</Application>
